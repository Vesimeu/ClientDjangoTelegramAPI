// Более Подробная Блок-Схема Сайта для Фриланс Заказов
digraph {
	A [label="Пользовательский Ввод"]
	B [label="Проверка Данных (JS)"]
	C [label="Отправка Формы (AJAX)"]
	D [label="Обработка Запроса (Django)"]
	E1 [label="Извлечение Данных"]
	E2 [label="Формирование Сообщения"]
	F [label="Связь с Telegram Bot"]
	G [label="Ответ Пользователю"]
	H [label="Уведомление Фрилансера"]
	I [label="Фрилансер Принимает Заказ"]
	J [label="Фрилансер Отклоняет Заказ"]
	A -> B
	B -> C
	C -> D
	D -> E1
	D -> E2
	E1 -> F
	E2 -> F
	F -> G
	F -> H
	H -> I
	H -> J
	I -> A [style=dashed]
	J -> A [style=dashed]
}
